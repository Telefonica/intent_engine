@prefix ns1: <http://example.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://example.org/intent/2> a ns1:Intent ;
    ns1:intentAdminState "ACTIVATED"^^xsd:string ;
    ns1:intentContext <http://example.org/intent/2/context/EnergyEfficiency>,
        <http://example.org/intent/2/context/IsolationLevel> ;
    ns1:intentExpectation <http://example.org/intent/2/expectation/1> ;
    ns1:intentPriority "1"^^xsd:int ;
    ns1:observationPeriod "60"^^xsd:int ;
    ns1:userLabel "green_nest"^^xsd:string .

<http://example.org/intent/2/context/EnergyEfficiency> a ns1:Context ;
    ns1:contextAttribute "EnergyEfficiency"^^xsd:string ;
    ns1:contextCondition "IS_EQUAL_TO_OR_GREATER_THAN"^^xsd:string ;
    ns1:contextValueRange "90%"^^xsd:string .

<http://example.org/intent/2/context/IsolationLevel> a ns1:Context ;
    ns1:contextAttribute "IsolationLevel"^^xsd:string ;
    ns1:contextCondition "IS_EQUAL_TO"^^xsd:string ;
    ns1:contextValueRange "DedicatedResources"^^xsd:string .

<http://example.org/intent/2/expectation/1> a ns1:Expectation ;
    ns1:expectationVerb "DELIVER"^^xsd:string ;
    ns1:intentContext <http://example.org/intent/2/expectation/1/context/AreaOfService>,
        <http://example.org/intent/2/expectation/1/context/ServiceType> ;
    ns1:objectInstance "Slice_001"^^xsd:string ;
    ns1:objectType "NEST"^^xsd:string .

<http://example.org/intent/2/expectation/1/context/AreaOfService> a ns1:Context ;
    ns1:contextAttribute "AreaOfService"^^xsd:string ;
    ns1:contextCondition "IS_EQUAL_TO"^^xsd:string ;
    ns1:contextValueRange "Urban"^^xsd:string .

<http://example.org/intent/2/expectation/1/context/ServiceType> a ns1:Context ;
    ns1:contextAttribute "ServiceType"^^xsd:string ;
    ns1:contextCondition "IS_EQUAL_TO"^^xsd:string ;
    ns1:contextValueRange "URLLC"^^xsd:string .

